{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NEXT_DISABLE_ESLINT": "1",
    "DISABLE_ESLINT_PLUGIN": "true",
    "NEXT_TYPESCRIPT_CHECK": "0",
    "NEXT_DISABLE_STATIC_GENERATION": "true"
  }
}
