
{
  "version": 2,
  "buildCommand": "node prebuild.js && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NEXT_DISABLE_ESLINT": "1",
    "DISABLE_ESLINT_PLUGIN": "true",
    "NEXT_TYPESCRIPT_CHECK": "0"
  },
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
